{
  "build": {
    "command": "npm run build",
    "destination": ".next",
    "root": ".",
    "environment": {
      "NODE_VERSION": "18",
      "NPM_VERSION": "9",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "production": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    },
    "preview": {
      "NODE_ENV": "preview",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}